{
  "name": "cytoscape-cx2js",
  "version": "0.2.1",
  "description": "A cx to Cytoscape JS utility",
  "repository": {
    "type": "git",
    "url": "https://github.com/cytoscape/cytoscape-cx2js.git"
  },
  "bugs": {
    "url": "https://github.com/cytoscape/cytoscape-cx2js/issues"
  },
  "homepage": "https://github.com/cytoscape/cytoscape-cx2js",
  "main": "./build/bundle.js",
  "scripts": {
    "prepublish": "run-s build",
    "lint:js": "eslint ./src",
    "bundle:js": "webpack",
    "watch:js": "webpack --watch",
    "watch:sync-bundle": "browser-sync start --config browser-sync.config.js",
    "bundle": "run-s bundle:*",
    "build": "run-p bundle",
    "build-prod": "cross-env NODE_ENV=production run-s build",
    "clean": "rimraf build/*",
    "lint": "run-s lint:*",
    "watch": "run-p watch:*",
    "test": "mocha"
  },
  "dependencies": {
    "fs": "0.0.2",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "cross-env": "^5.0.0",
    "browser-sync": "^2.18.13",
    "npm-run-all": "^4.1.1",
    "echo-cli": "^1.0.8",
    "rimraf": "^2.6.2",
    "webpack": "^4.17.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^8.0.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-env": "^1.6.0",
    "mocha": "^5.2.0",
    "chai": "^4.1.2",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "fs-extra": "^7.0.0"
  },
  "engines": {
    "node": ">=6.3.0"
  },
  "browserslist": "last 3 versions, >1%"
}
